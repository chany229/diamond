<h1>category: <%= @category.title %></h1>
<div class="content">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Title:</strong>
    <%= @category.title %>
  </p>

  <p>
    <strong>Father:</strong>
    <%= @category.father_id %>
  </p>

  <p>
    <strong>Position:</strong>
    <%= @category.position %>
  </p>
  <%= link_to 'Edit', edit_admin_category_path(@category) %> |
  <%= link_to 'Back', admin_categories_path %>
</div>
